public class Ex1 {
    public static void print (String hello) {
        System.out.println(hello);
    }

    public static void main(String[] args) {
        /*P1
        System.out.println("Hello World");

        P2
        String gmWorld = "Hello World";
        System.out.println(gmWorld); 
        */
        

        //P3
        String hello1 = "Hello";
        String world = "World";
        String helloWorld = hello1 + " " + world;
        System.out.println(helloWorld);
        
        //p4
        print(helloWorld);
    }

}
